{
  "env": {
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "extends": [
      "google",
      "react-app"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "require-jsdoc": 0,
      "max-len": ["error", { "code": 120 }],
      "new-cap": 0,
      "comma-dangle": ["error", "never"],
      "object-curly-spacing": 0
  }
}
